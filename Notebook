struct Notebook
{
    public string model;
    public string manufacturer;
    public int price;

    public Notebook(string model, string manufacturer, int price)
    {
        this.model = model;
        this.manufacturer = manufacturer;
        this.price = price;
    }

    public void DisplayInfo()
    {
        Console.WriteLine($"\nNotebook: {model}, Manufacturer: {manufacturer}, Price: {price}");
    }
}
